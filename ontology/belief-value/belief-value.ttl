@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix cidoc-inf: <http://www.ics.forth.gr/isl/CRMinf/>.


@base <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value>.
@prefix belief-value: <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#>.

<https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value>
	a owl:Ontology;
	dct:license <http://creativecommons.org/licenses/by/3.0/>;
	dct:title "An ontology about belief value"@en;
	dct:description """Formal description of belief value, general subclasses, related classes and properties. This ontology adopts the majority of the concepts of the unpublished ontology belief-value created in the project NIE-INE by Hans Cools and Roberta Padlina: https://github.com/nie-ine/Ontologies/blob/master/Nie-ontologies/Generic-ontologies/belief-value.ttl"""@en;
	dct:creator "Rebekka Plüss, research assistant and software developer, States Archive canton of Zürich"@en;
	dct:publisher "States Archive canton of Zürich"@en;
	owl:versionInfo "2021-11-26"^^xsd:date.

#
# CLASSES
#

belief-value:BeliefValue
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#BeliefValue>;
	a rdfs:Class;
	rdfs:label "belief value"@en, "Glaubenswert"@de;
	rdfs:comment """Value of a belief in a proposition by a certain agent in a certain time."""@en;
	rdfs:subClassOf cidoc-inf:I2_Belief;
	owl:oneOf (belief-value:Certain belief-value:Uncertain belief-value:VeryLikely belief-value:Likely belief-value:LessLikely belief-value:Neutral belief-value:LessUnlikely belief-value:Unlikely belief-value:VeryUnlikely belief-value:Impossible).

#
# INSTANCES
#

belief-value:Certain
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#Certain>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "certain"@en, "sicher"@de;
	rdfs:comment """Belief value representing being 'true'."""@en.

belief-value:Uncertain
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#Uncertain>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "uncertain"@en, "unsicher"@de;
	rdfs:comment """Belief value representing that being 'true' can not be assured."""@en.

belief-value:VeryLikely
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#VeryLikely>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "very likely"@en, "sehr warscheinlich"@de;
	rdfs:comment """Belief value representing a high probability of being true."""@en.

belief-value:Likely
	rdfs:isDefinedBy <hhttps://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#Likely>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "likely"@en, "warscheinlich"@de;
	rdfs:comment """Belief value representing a moderate probability of being true."""@en.

belief-value:LessLikely
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#LessLikely>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "less likely"@en, "wenig warscheinlich"@de;
	rdfs:comment """Belief value representing a low probability of being true."""@en.

belief-value:Neutral
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#Neutral>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "neutral"@en, "neutral"@de;
	rdfs:comment """Belief value representing being neither true nor false."""@en.

belief-value:LessUnlikely
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#LessUnlikely>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "less unlikely"@en, "wenig unwarscheinlich"@de;
	rdfs:comment """Belief value representing a low probability of being false."""@en.

belief-value:Unlikely
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#Unlikely>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "unlikely"@en, "unwarscheinlich"@de;
	rdfs:comment """Belief value representing a moderate probability of being false."""@en.

belief-value:VeryUnlikely
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#VeryUnlikely>;
	a rdfs:Class, belief-value:BeliefValue;
	rdfs:label "very unlikely"@en, "sehr unwarscheinlich"@de;
	rdfs:comment """Belief value representing a high probability of being false."""@en.

belief-value:Impossible
	rdfs:isDefinedBy <https://github.com/stazh/sw-ehedaten/tree/main/ontology/belief-value#Impossible>;
	a rdfs:Class;
	rdfs:label "impossible"@en, "unmöglich"@de;
	rdfs:comment """Belief value representing being 'false'."""@en.
